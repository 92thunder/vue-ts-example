{
  "name": "vue-ts-core",
  "version": "0.0.0",
  "main": "src/index.ts",
  "license": "MIT",
  "private": true,
  "scripts": {
    "test": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/test/**/*.ts?(x)"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts?(x)"
    ]
  },
  "devDependencies": {
    "@types/jest": "^23.1.1",
    "jest": "^23.1.0",
    "ts-jest": "^22.4.6"
  }
}
